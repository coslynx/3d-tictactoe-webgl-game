{
  "compilerOptions": {
    "target": "ESNext",
    "jsx": "react-jsx",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "strict": true,
    "skipLibCheck": true,
    "module": "ESNext",
    "resolveJsonModule": true,
    "declaration": true,
    "sourceMap": true,
    "types": ["jest"],
    "baseUrl": ".",
    "paths": {
      "@/src/*": ["./src/*"]
    },
    "incremental": true,
    "cache": ".tscache"
  },
  "include": ["src"],
  "exclude": ["node_modules"]
}